{
	"name": "vscode-github-issue-notebooks",
	"displayName": "GitHub Issue Notebooks",
	"description": "GitHub Issue Notebooks for VS Code",
	"publisher": "ms-vscode",
	"preview": true,
	"version": "0.0.130",
	"repository": {
		"url": "https://github.com/microsoft/vscode-github-issue-notebooks"
	},
	"engines": {
		"vscode": "^1.72.0-insider"
	},
	"categories": [
		"Other"
	],
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	},
	"icon": "icon.png",
	"activationEvents": [
		"onNotebook:github-issues"
	],
	"main": "./dist/extension-node.js",
	"l10n": "./l10n",
	"contributes": {
		"notebookRenderer": [
			{
				"id": "seed-renderer-simple",
				"entrypoint": "./dist/extension/simple-renderer.js",
				"displayName": "Simple Renderer",
				"mimeTypes": [
					"x-application/seed-output"
				]
			}
		],
		"notebooks": [
			{
				"type": "seed-notebook",
				"displayName": "seed Notebook",
				"selector": [
					{
						"filenamePattern": "*.seednb"
					}
				]
			}
		],
		"commands": [
			{
				"command": "seed-issues:new",
				"title": "New Seed notebook"
			}
		]
	},
	"scripts": {
		"lint": "eslint --config .eslintrc.json ./src/**/*.ts",
		"vscode:prepublish": "npm run webpack-compile",
		"compile": "webpack --mode none --no-cache",
		"webpack-watch": "webpack --mode none --watch",
		"ts-compile": "tsc -b",
		"integration-test": "npm run ts-compile && node ./out/test/test-integration/runTest.js",
		"unit-test": "npm run ts-compile && npx mocha ./out/test/test-unit",
		"compile-lint-test": "npm run ts-compile && npm run lint && npm run unit-test",
		"precommit": "npm run lint",
		"deploy": "npx vsce publish --noVerify"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "20.x",
		"@types/vscode": "^1.92.0",
		"@types/vscode-notebook-renderer": "^1.72.3",
		"@typescript-eslint/eslint-plugin": "^5.9.0",
		"@typescript-eslint/parser": "^5.9.0",
		"eslint": "^8.6.0",
		"glob": "^7.1.6",
		"husky": "^7.0.2",
		"mocha": "^9.1.3",
		"preact": "^10.4.6",
		"raw-loader": "^4.0.2",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"vscode-test": "^1.3.0",
		"webpack": "^5.88.2",
		"webpack-cli": "5.1.4",
		"util": "^0.12.5"
	},
	"dependencies": {
		"@octokit/rest": "^18.5.3",
		"abort-controller": "^3.0.0",
		"axios": "^1.7.4",
		"fast-xml-parser": "^4.4.1",
		"vscode-github-issue-notebooks": "file:",
		"wds-blog-search": "file:"
	}
}